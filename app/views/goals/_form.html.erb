<%= form_with model: @goal, local: true do |form| %>
  <div>
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>

  <div>
    <%= form.label :objectives %>
    <%= form.text_field :objectives %>
  </div>

  <div>
    <%= form.label :category %>
    <%= form.text_field :category %>
  </div>

  <div>
    <%= form.label :active %>
    <%= form.check_box :active %>
  </div>

  <%= form.submit %>
<% end %>
